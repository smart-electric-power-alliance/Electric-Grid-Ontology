nc:DRVENService
  rdf:type owl:Class ;
  rdfs:subClassOf nc:EnergyManagementService ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ar:isCollectionOf ;
      owl:someValuesFrom nc:BidPowerUsage ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ar:isCollectionOf ;
      owl:someValuesFrom nc:ControlLoad ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
