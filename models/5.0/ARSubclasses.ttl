<http://sv.cmu.edu/AWP/5.0/ARSubclasses>
      rdf:type owl:Ontology ;
      owl:imports <http://sv.cmu.edu/AWP/5.0/ActorRoles> .

<http://sv.cmu.edu/AWP/5.0/ActorRoles#(Energy)Dispatcher>
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "Actor Role in charge of dispatching Electricity (usually at the transmission level)"^^xsd:string ;
      ar:nistLegacyDomain "Transmission"^^xsd:string ;
      ar:type "Organizational Entity"^^xsd:string .

ar:AMIHeadEnd
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Provides the Interface to the Advanced Metering Infrastructure and manages the information exchanges between third party systems. The head-end is the command application for AMI solution. Optionally it manages required or desired meter reading schedules, and enacts other meter and communication-related commands. Most Head-ends also contain a vendor specific extension like a Meter Data Master (MDMS) and Validates, edits, and estimates (VEE) meter calculations."^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Device"^^xsd:string .

ar:AMIOperator
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "General operator of the AMI system"^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Role"^^xsd:string .

ar:AMIServiceEngineer
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "External actor responsible for the installation, operation, maintenance and de-installation of the system components. It may access, if properly identified and authorized, those components either directly, via local operation and maintenance interfaces, or from a system component from a higher hierarchical level (e.g. meters may be accessed for maintenance purposes via NNAPs or the HES)."^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Person"^^xsd:string .

<http://sv.cmu.edu/AWP/5.0/ActorRoles#AccessPoint(ie.DataAggregationPoint)>
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Collects and aggregates all data from/to end devices through Neighbourhood and/or Field Area and/or AMI networks. These devices are IEEE 802.11n compliant. They interface the Backhaul WAN."^^xsd:string ;
      ar:nistLegacyDomain "Distribution"^^xsd:string ;
      ar:type "Device"^^xsd:string .

ar:AccountManagement
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualService ;
      ar:description "Business function or service that manages supplier and customer business accounts. "^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:AccountManagementSystem
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Business function or service that manages supplier and customer business accounts. "^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:Actuator
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "An actuator is a transducer that accepts a signal and converts it to a physical action. In other words, an actuator causes an action to occur relating to the data that was sent to it. The are used to remotely operate devices such as switches and circuit-breakers."^^xsd:string ;
      ar:nistLegacyDomain "Cross Cutting"^^xsd:string ;
      ar:type "Device"^^xsd:string .

ar:AggregationResource
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualZone ;
      ar:description "A market-dependent group of Response Method Aggregations that represents a dispatchable entity."^^xsd:string ;
      ar:nistLegacyDomain "Market"^^xsd:string ;
      ar:type "Organizational Entity"^^xsd:string .

ar:Aggregator
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "The role that combines curtailment, demand or DER of two or more customers into a single purchasing unit to negotiate the purchase or the sale of electricity to retail electric providers. Aggregators may not sell or take title to electricity. Retail electric providers are not aggregators."^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:AncillaryOpsMarket
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "Provide a market to provide frequency support, voltage support, spinning reserve and other services as defined by FERC, NERC and the various ISO. These markets normally function on a regional or ISO basis. "^^xsd:string ;
      ar:nistLegacyDomain "Market"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:AncillaryServiceProviders
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Provide third-party ancillary services to provide frequency support, voltage support, spinning reserve and other services as defined by FERC, NERC and the variousttransmission authority,"^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:Appliance
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "generic appliance"^^xsd:string ;
      ar:nistLegacyDomain "Cross Cutting"^^xsd:string ;
      ar:type "Device"^^xsd:string .

ar:ApplianceControl
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "The role associated with the external control of an End Device."^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:ApplianceCustomer
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Consumer (in-home) electric device.  Potentially controllable (making them Customer Devices) to reduce/increase load as directed.  Includeslights,  pool pumps, air conditioners, electric water heaters, refrigerators, washers, electric dryers, dishwashers, &c."^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Device"^^xsd:string .

ar:AssetInvestmentPlanning
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "Asset investment planning involves strategy definition and prioritisation, maintenance strategy planning, risk management, programme management and decision-making. It drives the condition, configuration, performance, operating costs, and flexibility of the asset base, with the aim of maximising value."^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:BackOfficeSystems
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Applications which support the admistrative support for the utility or Service Provider.  These applcations usually include customer billing, general ledger & accounting"^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:BackhaulWAN
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Backhaul Wide-Area Networks (WAN) provides the telcommuncations facilities from edge devices to the Grid Operations Core Network (LAN) eitherdirectly or via a WAN. These edge devices include Substation, Field Automation Networks(FAN), Supervisory Control And Data Acquisition (SCADA) devices, dvanced Meter Infrastructure (AMI) networks, Distributed Generation Resources, etc. A backhaul WAN may be owned by a utility and/or managed by a telecom/cable Service Provider. It may be wireless, wireline, cable or fibre."^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Facility"^^xsd:string .

ar:BalanceSupplier
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "A party that markets the difference between actual metered energy consumption and the energy bought with firm energy contracts by the party connected to the grid. In addition the balance supplier markets any difference with the firm energy contract (of the party connected to the grid) and the metered production. "^^xsd:string ;
      ar:nistLegacyDomain "Market"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:BalancingAuthority
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "Ensures  the supply of electricity corresponds to the anticipated consumption of electricity during a given time period and financially regulates any imbalance that arises. THIS NEEDS UPDATE (USE NERC DEFN)"^^xsd:string ;
      ar:nistLegacyDomain "Market"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:Billing
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualService ;
      ar:description "Dept that calculates and produces consolidated customer bills, usually allows customer access via Customer Portal."^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:BiomassFacility
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Power plant that produces electricity by using biomass. Biomass includes usually refers to using methane gas produced by landfills, farm lots as fuel for the power generation facility. "^^xsd:string ;
      ar:nistLegacyDomain "Generation"^^xsd:string ;
      ar:type "Device"^^xsd:string .

ar:BuildingAutomationSystem
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "The Building Automation System (BAS), also commonly referred to as a Customer Energy Management System (EMS), controls the premise's electrical devices and provides status information.  Building automation systems can be used to allow users to make automated or manual decisions to control energy consumption more efficiently. Through the meter or gateway interface the BAS receives pricing and other signals for managing the building’s systems, including heating and cooling systems, lighting, DER, electric storage, PHEVs and appliances. The BAS may be a utility subscription service, a 3rd party offered service, a consumer specified policy, a consumer owned device, or a manual control by the utility or consumer."^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:BuildingManagement
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualService ;
      ar:description "Service that monitors and controls building energy and which potentially responds to signals while minimizing impact on building occupants. "^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:BusinessEntity
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualActor ;
      ar:description "The wholesale or retail entity that interacts with other entities in its market."^^xsd:string ;
      ar:nistLegacyDomain "Market"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:BusinessPlanningAndReporting
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualService ;
      ar:description "These actors perform strategic business modeling, manpower planning, reporting, account management, and both assess and report on risk, performance and business impact. "^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:Campus
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Set of co-located grounds and buildings operated by an entity such as a corporation.  Examples include, industrial parks, universities and corporate business parks,and supporting physical infrastructures"^^xsd:string ;
      ar:nistLegacyDomain "Cross Cutting"^^xsd:string ;
      ar:type "Facility"^^xsd:string .

ar:CapacitorBank
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Control  voltage  by minimizing voltage drops  and absorbing  line spikes. They also provide Volt/VAR control compensating for VAR losses for large inductive loads."^^xsd:string ;
      ar:nistLegacyDomain "Distribution"^^xsd:string ;
      ar:type "Device"^^xsd:string .

<http://sv.cmu.edu/AWP/5.0/ActorRoles#Clearing&SettlementAgent>
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "Assumes liability for clearing and/or settlement of contracts and provides contractual counterparty within a Power Exchangse and  Over the Counter (OTC) contracts."^^xsd:string ;
      ar:nistLegacyDomain "Market"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:CoalFacility
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Power plant that produces electricity steam by burning coal. They provide the most inexpensive method of producing power and are commonly for base-load and ancillary services."^^xsd:string ;
      ar:nistLegacyDomain "Generation"^^xsd:string ;
      ar:type "Facility"^^xsd:string .

ar:Cogeneration
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalRole ;
      ar:description "Devices which increase the efficiency of fuel by capturing and utilizing the both heat and power produced when the fuel is burned. Examples include combined cycle devices which utilize residual heat to generate additional electricity, topping cycle devices which use residual heat for heating or cooling, and bottom cycle processes which use waste heat to produce electricity."^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Device"^^xsd:string .

ar:CommercialCustomer
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "A business that  receives electricity from the electric distribution system, with usage below the threshold set for industrial usage. "^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:CommercialVendor
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "Comercial venodrs that provide solutions, services, and equipment "^^xsd:string ;
      ar:nistLegacyDomain "Cross Cutting"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:CommunicationMethod
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "The method by which an object communicates with another object to instruct, measure or control."^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Data"^^xsd:string .

ar:CommunicationNetworks
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalService ;
      ar:description "Wireline or wireless networks play a role in AMI, Distribution and Substation Automation, automation backhaul and EV roaming. This include Wireless Service Providers (WSP) now commonly used for AMI NANs, EV charging stations. "^^xsd:string ;
      ar:nistLegacyDomain "Cross Cutting"^^xsd:string ;
      ar:type "Facility"^^xsd:string .

ar:CommunicationsNetwork
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "The planning, operations and maintenance of all communications Network assets including cable, wire, fibre, routers, switches, wireless, etc required to support Operations. "^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Facility"^^xsd:string .

ar:CommunicationsNetworkOperations
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalService ;
      ar:description "The planning, operations and maintenance of all communications "^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:Construction
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualService ;
      ar:description "Examples of construction work include service installations, line extensions, and system betterment projects."^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:ConstructionSupervision
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalRole ;
      ar:description "Monitoring and management of construction work to minimize negative variances from planned costs, performance, and schedule."^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:ConsumptionResponseMethod
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "A measurable action taken in response to an instruction to change consumption."^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:ConsumptionResponsibleParty
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "A party who can be brought to rights, legally and financially, for any imbalance between energy bought and consumed for all associated metering points. "^^xsd:string ;
      ar:nistLegacyDomain "Market"^^xsd:string ;
      ar:type "Role"^^xsd:string .

ar:ControlAreaOperator
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description """Responsible for : 
1. The coordination of exchanges between its associated control blocks and the organisation of the coordination of exchange programs between its related control areas. 
2. The load frequency control within its own block and ensuring that its control areas respect their obligations in respect to load frequency control and time deviation. 
3. The organisation of the settlement and/or compensation between its control areas.
"""^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Role"^^xsd:string .

ar:ControllableLoad
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "Devices whose electric usage can be varied in response to signals. Examples include air conditioners, industrial devices and water heaters. May be controlled by a Facility BAS, a Facility Gateway, though a Smart Meter (serving as a Facility Gateway), or other means such as direct communication of price."^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Device"^^xsd:string .

ar:CoordinationCenterOperator
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description """Responsible for : 
1. The coordination of exchange programs between its related control blocks and for the exchanges between its associated coordination center zones. 
2. Ensuring that its control blocks respect their obligations in respect to load frequency control. 
3. Calculating the time deviation in cooperation with the associated coordination centers. 
4. Carrying out the settlement and/or compensation between its control blocks and against the other coordination center zones"""^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Role"^^xsd:string .

ar:CriticalLoads
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "Individual loads deemed to be critical to the operation of the facility or process."^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Device ?"^^xsd:string .

ar:CustomerDevice
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "Household devices that can react to remote management, whether to price, grid integrity, or other energy management signaling. May be controlled by a Facility Building Automation System (BAS), a Facility Gateway, though a Smart Meter (serving as a Facility Gateway), or other means such as direct communication of price or other information. Communication to the device might be via a Home-Area Network or other means."^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Device ? "^^xsd:string .

ar:CustomerInformationSystem
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "The CIS application provides Customer status information necessary to support Demand/Response programs, outage/restoration and usage information. The CIS provides the usage information for Billing. Optionally, CIS includes other functions including outage management, spatial information and Meter Data Managment System (MDMS) functions. CIS contains customer load data which is estimated for each nodal location on a feeder to provide load shapes for different load categories. The CIS fucntion can reside either with a Service Provider or a utility (which  fulfills that role, notably when there aren't any third-parties in the service territory)"^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:CustomerManagement
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualService ;
      ar:description "Utility function that manages customer relationships by providing point-of-contact (PoC) and resolution for customer issues and problems. "^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:CustomerMeter
      rdf:type owl:Class ;
      rdfs:subClassOf ar:PhysicalActor ;
      ar:description """Meters are devices which measure the amount of electricity used at a particular site. They are typically located at the customer facility and owned by the distributor (eg: utility) or retail provider. 
"""^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Device"^^xsd:string .

ar:CustomerPortal
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalService ;
      ar:description "Internet website so Customers can access their energy and cost information online, enrollment in prepayment electric services and enablement of third party monitoring and control of customer equipment."^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:CustomerPremiseNetwork
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalService ;
      ar:description "Customer Premise Networks support communcations between the various other actors found on a customer premise (eg: Appliances, Thermostats, Energy Management Systems); They are also known as Home Area Networks (HAN), Industrial Area Networks (IAN), Building Area Networks (BAN) and Customer Area Networks (CAN)"^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Facility"^^xsd:string .

ar:CustomerServiceRepresentative
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "Customer service provided by a person (e.g., sales and service representative), or by automated means called self-service (e.g., Interactive Voice Response (IVR))."^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Person"^^xsd:string .

ar:CustomerSubstation
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "Transmission or distribution substation located at the customer facilty to set-down power wiithin the customer facilty. Contains infrastructre to control/monitor and protect the distribution grid."^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Facility"^^xsd:string .

ar:CustomerSupport
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "Customer Support actors help customers to purchase, provision,schedule instalationl, troubleshoot power system services, and relay/record customer trouble reports. "^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:DemandManagement
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualService ;
      ar:description "DR issues pricing or other signals to appliances and equipment at customer locations in order to request customers (or their pre-programmed systems) to decrease or increase their loads in response to the signals. Needs to be crisper description - Ron Cunningham to do"^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:DemandResponseManagementSystem
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "DRMS issues pricing or other signals to appliances and equipment at customer locations in order to request customers (or their pre-programmed systems) to decrease or increase their loads in response to the signals."^^xsd:string ;
      ar:nistLegacyDomain "Operations"^^xsd:string ;
      ar:type "Application"^^xsd:string .

ar:DemandResponseObjects
      rdf:type owl:Class ;
      rdfs:subClassOf <http://sv.cmu.edu/AWP/5.0/ActorRoles#ConceptualNA Actor> ;
      ar:description "Physical and logical types of demand response resource objects. - Refer to NAESB for help - is this is a object model - call Marty Burns"^^xsd:string ;
      ar:nistLegacyDomain "Distribution"^^xsd:string ;
      ar:type "Data"^^xsd:string .

ar:DemandResponseProvider
      rdf:type owl:Class ;
      rdfs:subClassOf ar:LogicalActor ;
      ar:description "A service provider that either serves as an aggregator  or otherwise provides distribution of Demand/Response (DR) signals. "^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:DemandResponseServiceProvider
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualRole ;
      ar:description "A Demand Response (DR) Service Provider may serve as an aggregator (in the Market domain), or otherwise provide distribution of DR signals."^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Organization"^^xsd:string .

ar:DesignatedDispatchServiceEntity
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualService ;
      ar:description "A role service which carries the responsibility of receiving and processing demand resource dispatch instructions or market information and (optionally) providing response information.  "^^xsd:string ;
      ar:nistLegacyDomain "Service Provider"^^xsd:string ;
      ar:type "Service"^^xsd:string .

ar:DisplayUser
      rdf:type owl:Class ;
      rdfs:subClassOf ar:PhysicalActor ;
      ar:description "External actor interacting directly with the simple external consumer display."^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Person"^^xsd:string .

ar:DistributedGeneration
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualService ;
      ar:description "Distributed Generation, also called Distributed Energy Resources (DER), includes small-scale generation or storage of whatever form. This is in contrast to centralized or generation and/or storage of electricity. These generation facilities are part of Demand/Reponse programs and may be dispatchable resources. The primary distictions between Distributed Generation (DG) and Generation is: Generation is attached via Transmission facilities, output is sold in wholesale markets, provides base load; DG is sited on a Customer permises attached to the Distribution grid, output is Retail (unless sold via an aggregator), can provide ancillary services.  These generation facilities include but are not limited to Photvoltaic panel (PV), micro-hydro, mindmill, Plug-in Hybrid/Electric Cars (PHEV), and potentially Fuel cells. These facilites are usually not scheduled but can be dispatched."^^xsd:string ;
      ar:nistLegacyDomain "Customer"^^xsd:string ;
      ar:type "Device"^^xsd:string .

ar:DistributedGenerationAggregation
      rdf:type owl:Class ;
      rdfs:subClassOf ar:ConceptualService ;
      ar:description "Distributed  Generation Aggregators combine smaller participants (ie: providers, customers' distributed generation or curtailment assets) to enable these  participants access in larger markets as a virtualized trader"^^xsd:string ;
      ar:nistLegacyDomain "Market"^^xsd:string ;
      ar:type "Application"^^xsd:string .

